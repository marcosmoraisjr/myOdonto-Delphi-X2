  object qrySaldos: TADOQuery
    Connection = DM.Conexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT SALDOCONTASBANCARIAS.ID_SALDOCONTASBANCARIAS, SALDOCONTAS' +
        'BANCARIAS.ID_FILIAL, SALDOCONTASBANCARIAS.ID_CONTASBANCARIAS, '
      
        'SALDOCONTASBANCARIAS.ID_OPERADOR, SALDOCONTASBANCARIAS.ENTRADA_S' +
        'AIDA, SALDOCONTASBANCARIAS.HISTORICO, SALDOCONTASBANCARIAS.VLRSA' +
        'IDA, '
      
        'SALDOCONTASBANCARIAS.VLRENTRADA, SALDOCONTASBANCARIAS.DATA, CONT' +
        'ASBANCARIAS.DESCRICAO AS Banco FROM SALDOCONTASBANCARIAS '
      
        'INNER JOIN CONTASBANCARIAS ON SALDOCONTASBANCARIAS.ID_CONTASBANC' +
        'ARIAS = CONTASBANCARIAS.ID_CONTASBANCARIAS')
    Left = 664
    Top = 249
    object qrySaldosID_SALDOCONTASBANCARIAS: TAutoIncField
      FieldName = 'ID_SALDOCONTASBANCARIAS'
      ReadOnly = True
    end
