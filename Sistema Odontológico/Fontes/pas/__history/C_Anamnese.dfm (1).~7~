      ';Data Provider=NONE'
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SHAPE APPEND'
      'NEW adInteger IDAnamnese,'
      'NEW adInteger IDPergunta,'
      'NEW AdVarChar(60) Descricao,'
      'NEW adBoolean Obrigatorio')
