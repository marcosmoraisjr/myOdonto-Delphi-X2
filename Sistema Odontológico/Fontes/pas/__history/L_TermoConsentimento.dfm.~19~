  inherited qryPadrao: TADOQuery
    SQL.Strings = (
      
        'SELECT TERMOCONSENTIMENTO.ID_TERMOCONSENTIMENTO, TERMOCONSENTIME' +
        'NTO.ID_PACIENTE, TERMOCONSENTIMENTO.ID_MEDICO, TERMOCONSENTIMENT' +
        'O.RGRESPONSAVEL, TERMOCONSENTIMENTO.RESPONSAVEL,TERMOCONSENTIMEN' +
        'TO.DATA, MEDICOS.NOME AS Dentista, '
      
        'PACIENTE.ATIVO, PACIENTE.NOME AS Paciente, PACIENTE.RG FROM TERM' +
        'OCONSENTIMENTO '
      
        'INNER JOIN MEDICOS ON TERMOCONSENTIMENTO.ID_MEDICO = MEDICOS.ID_' +
        'MEDICO '
      
        'INNER JOIN PACIENTE ON TERMOCONSENTIMENTO.ID_PACIENTE = PACIENTE' +
        '.ID_PACIENTE'
      'ORDER BY TERMOCONSENTIMENTO.DATA')
