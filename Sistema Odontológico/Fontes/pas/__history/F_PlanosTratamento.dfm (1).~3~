  object qryPlanoTratamento: TADOQuery
    Connection = DM.Conexao
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      
        'SELECT PLANOTRATAMENTO.ID_PACIENTE, PLANOTRATAMENTO.ID_PLANOTRAT' +
        'AMENTO, PLANOTRATAMENTO.ID_MEDICO, PLANOTRATAMENTO.DTAINICIO, '
      
        'PLANOTRATAMENTO.DTAPREVISAOTERMINO, PLANOTRATAMENTO.DTATERMINO, ' +
        'PLANOTRATAMENTO.STATUS, PLANOTRATAMENTO.VALOR, MEDICOS.NOME As M' +
        'edico FROM PLANOTRATAMENTO'
      
        'INNER JOIN MEDICOS ON PLANOTRATAMENTO.ID_MEDICO = MEDICOS.ID_MED' +
        'ICO')
    Left = 568
    object qryPlanoTratamentoID_PLANOTRATAMENTO: TAutoIncField
      FieldName = 'ID_PLANOTRATAMENTO'
      ReadOnly = True
    end
    object qryPlanoTratamentoDTAINICIO: TDateTimeField
      FieldName = 'DTAINICIO'
      DisplayFormat = 'DD/MM/YYYY'
    end
    object qryPlanoTratamentoDTAPREVISAOTERMINO: TDateTimeField
      FieldName = 'DTAPREVISAOTERMINO'
      DisplayFormat = 'DD/MM/YYYY'
    end
    object qryPlanoTratamentoDTATERMINO: TDateTimeField
      FieldName = 'DTATERMINO'
      DisplayFormat = 'DD/MM/YYYY'
    end
    object qryPlanoTratamentoSTATUS: TStringField
      FieldName = 'STATUS'
    end
    object qryPlanoTratamentoVALOR: TFloatField
      FieldName = 'VALOR'
      DisplayFormat = '#,###,##0.00'
    end
    object qryPlanoTratamentoMedico: TStringField
      FieldName = 'Medico'
      Size = 255
    end
  end
  object dsPlanoTratamento: TDataSource
    DataSet = qryPlanoTratamento
    Left = 568
    Top = 32
  end
