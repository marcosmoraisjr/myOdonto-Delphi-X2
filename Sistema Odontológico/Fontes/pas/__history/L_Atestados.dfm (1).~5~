  inherited qryPadrao: TADOQuery
    Active = True
    SQL.Strings = (
      
        'SELECT PACIENTE.NOME AS Paciente, MEDICOS.NOME AS Dentista, OPER' +
        'ADORES.NOME AS Operador, ATESTADOS.ID_ATESTADOS,  ATESTADOS.OBSE' +
        'RVACAO,'
      
        'ATESTADOS.ID_PACIENTE, CASE FINS WHEN 0 THEN '#39'Escolares'#39' WHEN 1 ' +
        'THEN '#39'Trabalhistas'#39' END AS FINSS,ATESTADOS.ID_MEDICO, ATESTADOS.' +
        'ID_OPERADOR, ATESTADOS.DATA, ATESTADOS.FINS, ATESTADOS.HORAINI, ' +
        'ATESTADOS.HORAFIM FROM ATESTADOS '
      
        'INNER JOIN PACIENTE ON ATESTADOS.ID_PACIENTE = PACIENTE.ID_PACIE' +
        'NTE '
      'INNER JOIN MEDICOS ON ATESTADOS.ID_MEDICO = MEDICOS.ID_MEDICO '
      
        'INNER JOIN OPERADORES ON ATESTADOS.ID_OPERADOR = OPERADORES.ID_O' +
        'PERADORES')
    object qryPadraoPaciente: TStringField
      FieldName = 'Paciente'
      Size = 255
    end
    object qryPadraoDentista: TStringField
