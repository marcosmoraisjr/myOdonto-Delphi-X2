  inherited qryFotosPaciente: TADOQuery
    Connection = ImagemPaciente
    SQL.Strings = (
      
        'SELECT FOTOSPACIENTE.ID_FOTOSPACIENTE, FOTOSPACIENTE.FOTOS, FOTO' +
        'SPACIENTE.ID_PACIENTE,  FOTOSPACIENTE.DESCRICAO, FOTOSPACIENTE.D' +
        'ATA FROM PACIENTE'
      
        'INNER JOIN FOTOSPACIENTE ON PACIENTE.ID_PACIENTE = FOTOSPACIENTE' +
        '.ID_PACIENTE'
      'Order By Data')
    inherited qryFotosPacienteDATA: TDateTimeField
      EditMask = 'DD/MM/YYYY'
    end
